public class Aglobal
{
 int y=2;

}

public class Bglobal
{
 public void m2()
{
 Aglobal q = new Aglobal();
 int s=3;
 int u=s+(q.y);
 System.out.println(u);


}

}


public class Global
{
 public static void main(String args[])
{
 Aglobal c = new Aglobal();
 System.out.println(c.y);
 
 Bglobal b = new Bglobal();
 b.m2();


}

}
